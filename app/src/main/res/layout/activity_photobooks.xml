<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.erickogi14gmail.photozuri.Views.V1.Photobooks">

    <com.stepstone.stepper.StepperLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/stepperLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:ms_backButtonColor="@color/colorAccent"
        app:ms_completeButtonColor="@color/colorPrimary"
        app:ms_nextButtonColor="@color/colorAccent"
        app:ms_showErrorMessageEnabled="true"

        app:ms_stepperFeedbackType="content_progress|disabled_bottom_navigation"
        app:ms_stepperType="tabs"
        app:ms_tabNavigationEnabled="false" />
</android.support.constraint.ConstraintLayout>
